<!DOCTYPE html>
<html>
  <head>
    <title>AcheivementApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <body>
    <% if current_user %>
      <%= button_to "Logout", sessions_url, method: :delete %>
    <% else %>
      <%= button_to "Log In", new_sessions_url, method: :get %>
    <% end %>
    <%= yield %>
  </body>
</html>
